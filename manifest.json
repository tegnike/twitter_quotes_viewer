{
  "manifest_version": 3,
  "name": "Twitter Quotes Viewer",
  "version": "1.0",
  "description": "Adds a Quotes button to each tweet in the timeline",
  "permissions": [],
  "host_permissions": [
    "https://x.com/*",
    "https://twitter.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://x.com/*", "https://twitter.com/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "icons": {}
}
